<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-reverse-engineering PUBLIC "-//Hibernate/Hibernate Reverse Engineering DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-reverse-engineering-3.0.dtd">
<hibernate-reverse-engineering>
  <schema-selection match-catalog="UNICASE" match-schema="dbo"/>
  <table-filter match-name="Orders"/>
  <table-filter match-name="Categories"/>
  <table-filter match-name="Suppliers"/>
  <table-filter match-name="Customers"/>
  <table-filter match-name="Products"/>
  <table-filter match-name="OrderDetails"/>
</hibernate-reverse-engineering>
